<AnchorPane prefHeight="541.0"
            prefWidth="616.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ies.torredelrey.jfma.appgestionparking.controlador.menuController">
   <MenuBar layoutY="1.0" prefHeight="42.0" prefWidth="622.0">
     <menus>
       <Menu mnemonicParsing="false" text="App">
         <items>
               <CheckMenuItem fx:id="cmniCliente" mnemonicParsing="false" onAction="#OnClickCliente" text="Añadir Cliente" />
               <CheckMenuItem fx:id="cmniParking" mnemonicParsing="false" onAction="#OnClickParking1" text="Parking" />
               <CheckMenuItem fx:id="cmniFactura" mnemonicParsing="false" onAction="#OnClickFactura" text="Factura" />
               <MenuItem fx:id="mniSalir" mnemonicParsing="false" onAction="#OnClickSalir" text="Salir" />
         </items>
       </Menu>
     </menus>
   </MenuBar>
   <ImageView fx:id="imgAlcazar" fitHeight="451.0" fitWidth="622.0" layoutY="107.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../../imagenes/home.jpg" />
      </image>
   </ImageView>
 <AnchorPane fx:id="PanelCliente" layoutY="34.0" minHeight="0.0" minWidth="0.0" prefHeight="494.0" prefWidth="622.0">
      <children>
         <Button fx:id="btnGuardarClientes" layoutX="128.0" layoutY="455.0" mnemonicParsing="false" onAction="#guardaClientes" text="Guardar" />
         <Button fx:id="btnCancelarClientes" layoutX="381.0" layoutY="455.0" mnemonicParsing="false" onAction="#cancelarClientes" text="Cancelar" />
         <TextField fx:id="txtNombreCliente" layoutX="97.0" layoutY="74.0" />
         <Label layoutX="24.0" layoutY="78.0" text="NOMBRE:" />
         <Label alignment="CENTER" layoutX="120.0" layoutY="26.0" prefHeight="35.0" prefWidth="381.0" text="CLIENTES">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <TextField fx:id="txtApellidosCliente" layoutX="97.0" layoutY="115.0" />
         <Label layoutX="24.0" layoutY="119.0" text="APELLIDOS:" />
         <TextField fx:id="txtDniCliente" layoutX="97.0" layoutY="154.0" />
         <Label layoutX="24.0" layoutY="158.0" text="DNI:" />
         <Label layoutX="24.0" layoutY="197.0" text="FECHA NACIMIENTO" />
         <TextField fx:id="txtDireccionCliente" layoutX="97.0" layoutY="234.0" />
         <Label layoutX="24.0" layoutY="238.0" text="DIRECCIÓN:" />
         <TextField fx:id="txtTelefonoCliente" layoutX="427.0" layoutY="74.0" />
         <Label layoutX="354.0" layoutY="78.0" text="TELÉFONO:" />
         <DatePicker fx:id="dpFechaNacCliente" layoutX="147.0" layoutY="193.0" />
         <TextField fx:id="txtEmailCliente" layoutX="97.0" layoutY="277.0" />
         <Label layoutX="24.0" layoutY="281.0" text="EMAIL:" />
      </children>
   </AnchorPane>

</AnchorPane>
