<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="panelRoles" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ies.torredelrey.jfma.appgestionparking.controlador.rolesController">
   <children>
      <HBox alignment="TOP_CENTER" layoutX="34.0" layoutY="118.0" prefHeight="117.0" prefWidth="531.0">
         <children>
            <Button fx:id="rolAdmin" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#selectorBoton" prefHeight="115.0" prefWidth="178.0" text="Administrador" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
               <graphic>
                  <ImageView fx:id="imagenAdmin" fitHeight="116.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button fx:id="rolRecepcionista" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#selectorBoton" prefHeight="114.0" prefWidth="177.0" text="Recepcionista" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
               <graphic>
                  <ImageView fx:id="imagenRecepcionista" fitHeight="116.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button fx:id="rolClientes" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#btnRolClientes" prefHeight="114.0" prefWidth="178.0" text="Clientes" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
               <graphic>
                  <ImageView fx:id="imgClientes" fitHeight="116.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
