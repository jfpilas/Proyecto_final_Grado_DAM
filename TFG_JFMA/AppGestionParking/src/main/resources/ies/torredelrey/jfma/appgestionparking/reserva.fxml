<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ies.torredelrey.jfma.appgestionparking.controlador.reservaController">
   <children>
      <Label alignment="CENTER" layoutX="161.0" layoutY="14.0" text="HAZ TU RESERVA" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="146.0" layoutY="97.0" text="DNI:" />
      <TextField fx:id="txtDni" layoutX="292.0" layoutY="93.0" promptText="Escriba aquí su dni" />
      <Label layoutX="150.0" layoutY="294.0" text="FECHA DE SALIDA:" />
      <Label layoutX="145.0" layoutY="246.0" text="FECHA DE ENTRADA:" />
      <Label layoutX="145.0" layoutY="191.0" text="NÚMERO DE PLAZA:" />
      <TextField fx:id="txtNumPlaza" layoutX="292.0" layoutY="187.0" />
      <Label layoutX="146.0" layoutY="141.0" text="MATRÍCULA:" />
      <TextField fx:id="txtMatricula" layoutX="292.0" layoutY="137.0" promptText="Escriba aquí la matrícula de su coche" />
      <DatePicker fx:id="dtpFechaE" layoutX="280.0" layoutY="242.0" />
      <DatePicker fx:id="dtpFechaS" layoutX="280.0" layoutY="290.0" />
      <Button fx:id="btnReservar" layoutX="161.0" layoutY="350.0" mnemonicParsing="false" onAction="#OnClickReservar" text="Reservar" />
      <Button fx:id="btnCancelar" layoutX="390.0" layoutY="350.0" mnemonicParsing="false" onAction="#OnClickCancelar" text="Cancelar" />
   </children>
</AnchorPane>
